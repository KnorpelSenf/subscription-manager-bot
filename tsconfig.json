{
  "compilerOptions": {
    "charset": "utf8",
    "forceConsistentCasingInFileNames": true,
    "keyofStringsOnly": true,
    "module": "commonjs",
    "newLine": "lf",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "strict": true,
    "target": "es6",
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["index.ts"],
  "exclude": []
}
